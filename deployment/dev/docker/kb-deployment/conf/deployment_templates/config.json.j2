{
    "deploy": {
        "environment": "{{ kbase_ui.deploy_environment }}",
        "hostname": "{{ kbase_ui.deploy_hostname }}",
        "icon": "{{ kbase_ui.deploy_icon }}",
        "name": "{{ kbase_ui.deploy_name }}",
        "services": {
            "urlBase": "https://{{ kbase_ui.deploy_hostname }}"
        }
    },
    "ui": {
        "backupCookie": {
            "domain": "{{ kbase_ui.ui_backupcookie_domain }}",
            "enabled": {{ kbase_ui.ui_backupcookie_enabled }}
        },
        "services": {
            "analytics": {
                "google": {
                    "hostname": "{{ kbase_ui.ui_services_analytics_google_hostname }}",
                    "code": "{{ kbase_ui.ui_services_analytics_google_code }}"
                }
            }
        },
        "allow": {{ kbase_ui.allow }}
    },
    "services": {
        "narrative": {
            "url": "{{ kbase_ui.services_narrative_url }}"
        }
    }
}
